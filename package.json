{
	"name": "quillerbee",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "rollup -c -w",
		"routify": "routify",
		"dev:routify": "run-p dev routify",
		"fix": "pnpx eslint --fix \"{,!(node_modules|public|.routify)/**/}*.{js,svelte}\"",
		"format": "pnpx prettier --write --plugin-search-dir=. \"{,!(node_modules|public|.routify)/**/}*.{js,svelte}\"",
		"prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
		"build": "routify -b && rollup -c",
		"start": "sirv public --single"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/eslint-parser": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@fullhuman/postcss-purgecss": "^4.0.2",
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-replace": "^2.4.1",
		"@roxi/routify": "^2.14.0",
		"@tailwindcss/jit": "^0.1.4",
		"autoprefixer": "^10.2.5",
		"cssnano": "^4.1.10",
		"eslint": "^7.22.0",
		"eslint-plugin-svelte3": "^3.1.2",
		"npm-run-all": "^4.1.5",
		"pnpm": "^5.18.6",
		"postcss": "^8.2.8",
		"postcss-import": "^14.0.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"rollup": "^2.3.4",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"svelte": "^3.0.0",
		"svelte-preprocess": "^4.6.9",
		"tailwindcss": "^2.0.3"
	},
	"dependencies": {
		"@urql/svelte": "^1.2.0",
		"chroma-js": "^2.1.1",
		"graphql": "^15.5.0",
		"loglevel": "^1.7.1",
		"sirv-cli": "^1.0.0",
		"timeago.js": "^4.0.2",
		"typewriter-effect": "^2.17.0"
	}
}
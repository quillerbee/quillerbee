{
	"name": "quillerbee",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"routify": "routify",
		"fix": "pnpx eslint --fix \"{,!(node_modules|public|dist|.routify)/**/}*.{js,svelte}\"",
		"format": "pnpx prettier --write --plugin-search-dir=. \"{,!(node_modules|public|dist|.routify)/**/}*.{js,svelte,json,html}\"",
		"prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
		"build": "routify -b && vite build",
		"serve": "vite preview"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/eslint-parser": "^7.13.10",
		"@roxi/routify": "^2.15.1",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.5",
		"@tailwindcss/jit": "^0.1.7",
		"autoprefixer": "^10.2.5",
		"eslint": "^7.22.0",
		"eslint-plugin-svelte3": "^3.1.2",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.35.0",
		"tailwindcss": "^2.1.1",
		"vite": "^2.1.0",
		"vite-aliases": "^0.5.1"
	},
	"dependencies": {
		"@felte/reporter-tippy": "^0.2.7",
		"@felte/validator-yup": "^0.2.3",
		"@tailwindcss/typography": "^0.4.0",
		"@tailwindcss/forms": "^0.3.2",
		"@urql/svelte": "^1.2.0",
		"chroma-js": "^2.1.1",
		"country-emoji": "^1.5.6",
		"currency-symbol-map": "^5.0.1",
		"felte": "^0.6.6",
		"graphql": "^15.5.0",
		"loglevel": "^1.7.1",
		"marked": "^2.0.1",
		"slim-select": "^1.27.0",
		"sveltejs-tippy": "^3.0.0",
		"timeago.js": "^4.0.2",
		"typewriter-effect": "^2.17.0",
		"yup": "^0.32.9"
	}
}
